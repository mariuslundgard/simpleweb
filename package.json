{
  "name": "simpleweb",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "scripts": {
    "build": "npm run build-client && npm run build-server && npm run build-worker",
    "build-client": "cross-env NODE_ENV=production rollup --config .rollup/client.config.js",
    "build-server": "cross-env NODE_ENV=production rollup --config .rollup/server.config.js",
    "build-worker": "cross-env NODE_ENV=production rollup --config .rollup/worker.config.js",
    "clean": "rimraf dist",
    "clean-install": "rimraf node_modules package-lock.json && npm install",
    "dev": "cross-env NODE_ENV=development node scripts/dev",
    "docker-build": "docker build -t mariuslundgard/simpleweb:latest .",
    "docker-run": "docker push mariuslundgard/simpleweb:latest",
    "format": "prettier-eslint --write \"{.rollup,scripts,src}/**/*.js\"",
    "lint": "eslint .",
    "prebuild": "npm run clean",
    "precommit": "npm run lint && npm run typecheck && npm test",
    "prewatch": "npm run clean",
    "start": "cross-env NODE_ENV=production node scripts/start",
    "test": "jest",
    "typecheck": "flow check"
  },
  "author": "Marius Lundg√•rd <studio@mariuslundgard.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mariuslundgard/simpleweb.git"
  },
  "bugs": {
    "url": "https://github.com/mariuslundgard/simpleweb/issues"
  },
  "homepage": "https://github.com/mariuslundgard/simpleweb#readme",
  "engineStrict": true,
  "engines": {
    "node": "9.11.1"
  },
  "dependencies": {
    "archieml": "^0.4.2",
    "compression": "^1.7.2",
    "cross-env": "^5.1.4",
    "date-fns": "^1.29.0",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2",
    "html-entities": "^1.2.1",
    "htmlparser2": "^3.9.2",
    "jsonwebtoken": "^8.2.1",
    "lodash.camelcase": "^4.3.0",
    "preact": "^8.2.7",
    "preact-render-to-string": "^3.7.0",
    "qs": "^6.5.2",
    "redis": "^2.8.0",
    "request": "^2.85.0",
    "uuid": "^3.2.1"
  },
