{
  "name": "simpleweb",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "scripts": {
    "build": "npm-run-all build-*",
    "build-client-js": "cross-env NODE_PATH=src/lib NODE_ENV=production rollup --config .rollup/client.config.js",
    "build-client-css": "cross-env NODE_PATH=src/lib NODE_ENV=production node scripts/build-css.js",
    "build-server": "cross-env NODE_PATH=src/lib NODE_ENV=production rollup --config .rollup/server.config.js",
    "build-worker": "cross-env NODE_PATH=src/lib NODE_ENV=production rollup --config .rollup/worker.config.js",
    "clean": "rimraf dist",
    "clean-install": "rimraf node_modules package-lock.json && npm install",
    "dev": "cross-env NODE_PATH=src/lib NODE_ENV=development node scripts/dev",
    "docker-build": "scripts/docker-build.sh",
    "docker-push": "scripts/docker-push.sh",
    "format": "prettier-eslint --write \"{.rollup,scripts,src}/**/*.js\"",
    "lint": "eslint .",
    "prebuild": "npm run clean",
    "precommit": "npm run lint && npm run typecheck && npm test",
    "prewatch": "npm run clean",
    "start": "cross-env NODE_PATH=src/lib NODE_ENV=production node scripts/start",
    "test": "cross-env NODE_PATH=src/lib jest --verbose",
    "typecheck": "flow check"
  },
  "author": "Marius Lundg√•rd <studio@mariuslundgard.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mariuslundgard/simpleweb.git"
  },
  "bugs": {
    "url": "https://github.com/mariuslundgard/simpleweb/issues"
  },
  "homepage": "https://github.com/mariuslundgard/simpleweb#readme",
  "engineStrict": true,
  "engines": {
    "node": "10.4.1"
  },
  "dependencies": {
    "archieml": "^0.4.2",
    "body-parser": "^1.18.3",
    "compression": "^1.7.2",
    "cross-env": "^5.2.0",
    "date-fns": "^1.29.0",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2",
    "html-entities": "^1.2.1",
    "htmlparser2": "^3.9.2",
    "http-auth": "^3.2.3",
    "jsonwebtoken": "^8.3.0",
    "lodash.camelcase": "^4.3.0",
    "pino": "^4.17.3",
    "preact": "^8.2.7",
    "preact-redux": "^2.0.3",
    "preact-render-to-string": "^3.7.0",
    "qs": "^6.5.2",
    "redis": "^2.8.0",
    "redux": "^4.0.0",
    "request": "^2.87.0",
    "superagent": "^3.8.3",
    "uuid": "^3.2.1"
